    "# n 입력\n",
    "n = int(input())\n",
    "\n",
    "# n개의 정수 입력받기\n",
    "array = []\n",
    "for i in range(n):\n",
    "    array.append(int(input()))\n",
    "    \n",
    "# 파이썬 기본 정렬 라이브러리 이용하여 정렬 수행\n",
    "array = sorted(array, reverse=True)\n",
    "\n",
    "for i in array:\n",
    "    print(i, end=' ')"
   ]
  }
